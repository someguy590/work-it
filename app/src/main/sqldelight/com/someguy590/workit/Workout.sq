CREATE TABLE Workout (
id INTEGER PRIMARY KEY,
name TEXT NOT NULL,
weight TEXT NOT NULL,
sets INTEGER NOT NULL
);

insert:
INSERT INTO Workout(name, weight, sets) VALUES (?, ?, ?);

selectAll:
SELECT * FROM Workout;

update:
UPDATE Workout SET name = ?, weight = ?, sets = ? WHERE id = ?;

delete:
DELETE FROM Workout WHERE id = ?;

lastInsertRowId:
SELECT last_insert_rowid();
